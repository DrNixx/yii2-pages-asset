{
    "name": "drnixx/yii2-pages-asset",
    "description": "Yii2 Assets Bundle for Revox.Io Pages",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap4": "~1.0.0",
        
        "drnixx/yii2-fa-asset": "^1.0",
        "drnixx/yii2-unveil-asset": "^1.0",
        "drnixx/yii2-scrollbar-asset": "^1.0",
        "drnixx/yii2-mousewheel-asset": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Panteleeff",
            "email": "drnix@chess-online.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "onix\\": "src/"
        }
    },
     "config": {
        "platform": {"php": "5.6"}
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
